    "# 边玩游戏边学习\n",
    "\n",
    "def game():\n",
    "    for idx in range(5):\n",
    "        yield print(\"玩游戏。。。很开心。。。\")\n",
    "\n",
    "def sutdy():\n",
    "    for idx in range(5):\n",
    "        yield print(\"helloworld！\")\n",
    "\n",
    "for idx in range(5):\n",
    "    next(game())\n",
    "    next(sutdy())"
